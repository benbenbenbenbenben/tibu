{
  "name": "tibu",
  "version": "1.0.0",
  "description": "tibu is a low fuss high fun PEG parser generator/DSL written in TypeScript",
  "type": "module",
  "source": "src/tibu.ts",
  "exports": {
    "require": "./dist/tibu.cjs",
    "default": "./dist/tibu.modern.js"
  },
  "main": "./dist/tibu.cjs",
  "module": "./dist/tibu.module.js",
  "unpkg": "./dist/tibu.umd.js",
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch",
    "test": "vitest --global"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benbenbenbenbenben/tibu.git"
  },
  "keywords": [
    "peg",
    "pegs",
    "parser-generator",
    "typescript",
    "jison"
  ],
  "author": "Benjamin Babik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benbenbenbenbenben/tibu/issues"
  },
  "homepage": "https://github.com/benbenbenbenbenben/tibu#readme",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@vitest/ui": "^0.9.2",
    "microbundle": "^0.14.2",
    "typescript": "^4.6.3",
    "vitest": "^0.9.2"
  }
}
